{
  "name": "moderntribe/tribe-storage-aws-s3",
  "description": "Tribe Storage Adapter for AWS S3 SDK v3.x",
  "keywords": [
    "moderntribe",
    "tribe-storage",
    "tribe-storage-adapter",
    "flysystem",
    "s3",
    "aws-s3",
    "wordpress"
  ],
  "authors": [
    {
      "name": "Justin Frydman",
      "role": "Developer"
    },
    {
      "name": "Modern Tribe",
      "email": "admin@tri.be",
      "homepage": "https://tri.be"
    }
  ],
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:moderntribe/tribe-storage.git"
    }
  ],
  "require": {
    "php": ">=7.4",
    "php-di/php-di": "^6.0",
    "moderntribe/tribe-storage": "^3.0",
    "league/flysystem-aws-s3-v3": "^1.0"
  },
  "require-dev": {
    "brain/monkey": "2.*",
    "phpunit/phpunit": "^9",
    "php-mock/php-mock-mockery": "^1.3",
    "moderntribe/coding-standards": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "Tribe\\Storage\\Adapters\\": "src"
    },
    "files": [
      "plugin.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tribe\\Storage\\Adapters\\Tests\\": "tests"
    },
    "files": [
      "tests/Bootstrap.php"
    ]
  },
  "config": {
    "platform": {
      "php": "7.4.0"
    },
    "allow-plugins": {
      "cweagans/composer-patches": true,
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
